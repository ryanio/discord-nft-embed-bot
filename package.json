{
  "name": "discord-nft-embed-bot",
  "version": "2.1.0",
  "description": "A discord.js bot that listens to messages in channels and replies with items from an nft collection.",
  "author": "Ryan Ghods <ryan@ryanio.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "postinstall": "yarn build",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "start:dev": "ts-node src/index.ts",
    "format": "ultracite format --unsafe && npx tsc --noEmit",
    "lint": "ultracite lint && npx tsc --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "ethers": "^6.9.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^29.1.1",
    "@biomejs/biome": "2.2.2",
    "@types/node": "^18.0.0",
    "lint-staged": "^16.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "ultracite": "5.2.9"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "yarn dlx ultracite format"
    ]
  }
}
